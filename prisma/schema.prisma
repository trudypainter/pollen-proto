datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model ChannelCurated {
  id              Int       @default(autoincrement()) @id
  channelId       Int
  curatedIds      String
}